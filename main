#include <stdio.h>
#include <stdlib.h>

#define MAX_NUMBER_OF_STEPS <>
#define MAX <>

int main()
{
	FILE *inpf;
	char maze[MAX][MAX]
	int nl = 0; 
  	int nc = 0;

	If (inpf == NULL)
	{
		printf(“ERROR:File maze.txt cannot be opened”);
		exit(1);
	}
	//puts the file into the array MAZE
    	if ((f = fopen ("maze.txt", "r")) == NULL)
  	{
	}
	else {
		while ((c = fgetc(f)) != EOF)
 		{
		 	Maze [nl][nc] = c;
		 	if (c == '\n')
 			{	
		    		nl++;
		    		nc = 0;
	 	  	}
		  	else
			{
     		  	nc++;
			}
		}
	}
	//prints out the maze
	printf ("\nStart: ------------\n");
	for (int i = 0; i < MAX; i++)
	{
		for (int j = 0; j < MAX; j++)
		{
		   printf ("%c", Maze[i][j]);
		}
	}
	printf ("\n------------\n");

}//end of main file
